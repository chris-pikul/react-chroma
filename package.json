{
  "name": "react-chroma",
  "version": "1.0.0",
  "description": "Basic react UI framework with loosely opinionated design language applied",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisPikul510/react-chroma.git"
  },
  "keywords": [
    "react",
    "ui",
    "framework"
  ],
  "author": "Chris Pikul <ChrisPikul510@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChrisPikul510/react-chroma/issues"
  },
  "homepage": "https://github.com/ChrisPikul510/react-chroma#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "html-webpack-plugin": "^4.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
